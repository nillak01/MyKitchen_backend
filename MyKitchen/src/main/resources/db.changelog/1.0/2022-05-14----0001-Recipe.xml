<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet id="2022-04-15----00001-Recipe" author="K_Yaroslav">
        <preConditions>
            <not>
                <tableExists tableName="Recipe"/>
            </not>
        </preConditions>

        <createTable tableName="Recipe">
            <column name="id"
                    type="NUMERIC(19,0)"
                    autoIncrement="true">
                <constraints nullable="false"
                             primaryKey="true"/>

            </column>

            <column name="name"
                    type="VARCHAR(100)">
                <constraints nullable="false"/>

            </column>
            <column name="my"
                    type="BOOL">

                <constraints nullable="false"></constraints>

            </column>

            <column name="point_1"
                    type="VARCHAR(100)">
                <constraints nullable="true"/>

            </column>
            <column name="point_2"
                    type="VARCHAR(100)">
                <constraints nullable="true"/>

            </column>
            <column name="point_3"
                    type="VARCHAR(100)">
                <constraints nullable="true"/>

            </column>
            <column name="point_4"
                    type="VARCHAR(100)">
                <constraints nullable="true"/>

            </column>
            <column name="point_5"
                    type="VARCHAR(100)">
                <constraints nullable="true"/>

            </column>
            <column name="point_6"
                    type="VARCHAR(100)">
                <constraints nullable="true"/>

            </column>
            <column name="point_7"
                    type="VARCHAR(100)">
                <constraints nullable="true"/>

            </column>
            <column name="point_8"
                    type="VARCHAR(100)">
                <constraints nullable="true"/>

            </column>
            <column name="point_9"
                    type="VARCHAR(100)">
                <constraints nullable="true"/>

            </column>
            <column name="point_10"
                    type="VARCHAR(100)">
                <constraints nullable="true"/>

            </column>

        </createTable>
    </changeSet>

</databaseChangeLog>